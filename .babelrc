{
  "presets": [
    ["@babel/preset-env", { "modules": false }],
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "test": {
      "presets": ["@babel/env", "@babel/react"],
      "plugins": [
        "dynamic-import-node",
        "@babel/plugin-proposal-class-properties"
      ]
    }
  }
}
